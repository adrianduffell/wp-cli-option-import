{
    "name": "aduffell/option-import",
    "description": "Import WordPress options from a .yml file.",
    "type": "wp-cli-package",
    "homepage": "https://github.com/aduffell/wp-cli-option-import",
    "license": "MIT",
    "authors": [],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "files": [ "command.php", "src/functions.php" ]
    },
    "require": {
        "wp-cli/wp-cli": "^1.1.0",
        "symfony/yaml": "^2.7|^3.0"
    },
    "require-dev": {
        "behat/behat": "~2.5",
        "squizlabs/php_codesniffer": "^3.2",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.4.4",
        "automattic/phpcs-neutron-standard": "^1.2",
        "wp-coding-standards/wpcs": "^0.14.1"
    },
    "scripts": {
        "phpcs" : "./vendor/bin/phpcs -s",
        "test": [
            "@phpcs"
    ]
  }
}
